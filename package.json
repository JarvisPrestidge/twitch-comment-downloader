    },
    "devDependencies": {
        "@types/node": "^13.1.1",
        "auto-changelog": "^1.16.2",
        "prettier": "^1.19.1",
        "rimraf": "^3.0.0",
        "ts-node": "^8.5.4",
        "tslint": "^5.20.1",
        "tslint-config-prettier": "^1.18.0",
        "tslint-plugin-prettier": "^2.1.0",
        "typescript": "^3.7.4"
